{
  "name": "threejs-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate of three.js (use TypeScript)",
  "scripts": {
    "start": "webpack serve --mode development --config webpack.dev.js  --progress",
    "dev": "webpack --config webpack.dev.js --watch",
    "build": "yarn lint && webpack --config webpack.prod.js",
    "lint": "eslint src/ts --ext .ts",
    "lint:fix": "eslint src/ts --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/36t/threejs-boilerplate"
  },
  "license": "ISC",
  "homepage": "https://github.com/36t/threejs-boilerplate#readme",
  "dependencies": {
    "three": "^0.126.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@types/three": "^0.125.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.9.0",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^5.1.2",
    "image-webpack-loader": "^7.0.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^11.0.1",
    "shader-loader": "^1.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "ts-shader-loader": "^1.0.6",
    "typescript": "4.1.5",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
